<body class="showcase">
    <div class="container-fluid mb-2 mt-5 text-center ffr">
        <div class="container shadow-lg bg-white rounded text-center">
            <div class="row">
                <table class="table table-striped table-white text-center">
                    <tr>
                        <th>Email</th>
                        <th>Nome</th>
                        <th>Senha</th>
                    </tr>
                    <% for(i = 0;i<busca.length;i++) { %>
                    <tr>
                        <th><%=busca[i].email %></th>
                        <th><%=busca[i].nome %></th>
                        <th><%=busca[i].senha %></th>
                    </tr>
                    <% }%>
                </table>
            </div>
        </div>
    </div>
</body>

</html>